{
  "name": "sass-variables-util",
  "version": "1.0.0",
  "description": "A small wrapper around sass-variables for use during builds.",
  "main": "index.js",
  "scripts": {
    "changelog-major": "build-changelog --major",
    "changelog-minor": "build-changelog --minor",
    "changelog-patch": "build-changelog --patch",
    "cover": "istanbul cover --report cobertura --print detail tape -- test/index.js",
    "lint": "lint-trap",
    "test": "npm run lint -s && npm run cover -s",
    "fast-test": "npm run lint && tape test/index.js",
    "view-cover": "istanbul report html && opn ./coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freeqaz/sass-variables-util.git"
  },
  "keywords": [
    "sass",
    "variables",
    "test",
    "build"
  ],
  "author": "freeqaz@freeqaz.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freeqaz/sass-variables-util/issues"
  },
  "homepage": "https://github.com/freeqaz/sass-variables-util#readme",
  "devDependencies": {
    "build-changelog": "^2.1.2",
    "istanbul": "^0.3.13",
    "lint-trap": "^1.0.1",
    "opn": "^1.0.1",
    "tape": "^4.0.0"
  },
  "dependencies": {
    "sass-variables": "1.0.0",
    "xtend": "^4.0.0"
  }
}
